[
	{
		"name": "ems_http_server",
		"comment": "Catalog for ems_http_server",
		"owner": "emsbus",
		"version": "1.0.0",
		"service" : "ems_http_server:start",
		"url": "/emsbus/ems_http_server",
		"type": "KERNEL",
		"lang" : "erlang",
		"pool_size" : 1,
		"pool_max" : 1,
		"protocol" : "http",
		"tcp_max_connections" : 100000,
		"tcp_port": 2301,
		"tcp_keepalive": true,
		"tcp_nodelay": true,
		"tcp_accept_timeout" : 12000,
		"tcp_send_timeout" : 12000,
		"tcp_backlog" : 1024,
		"tcp_buffer" : 8000,
		"tcp_delay_send" : false,
		"start_timeout" : 4000,
		"enable" : true
	}
]
